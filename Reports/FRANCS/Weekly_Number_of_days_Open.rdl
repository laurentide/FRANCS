  <DataSets>
    <DataSet Name="NCF">
      <Fields>
        <Field Name="NCF_No">
          <DataField>NCF No</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Issue_date">
          <DataField>Issue date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Days_Open">
          <DataField>Days_Open</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Issued_By">
          <DataField>Issued By</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Assigned_To">
          <DataField>Assigned To</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Status_EN">
          <DataField>Status_EN</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Status_FR">
          <DataField>Status_FR</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>NCF</DataSourceName>
        <CommandText>SELECT     a.NCFNo AS [NCF No], CONVERT(char, a.IssueDate, 112) AS [Issue date], (DATEDIFF(dd, a.IssueDate, GETDATE()) + 1) - DATEDIFF(wk, a.IssueDate, 
                      GETDATE()) * 2 - (CASE WHEN DATENAME(dw, IssueDate) = 'Sunday' THEN 1 ELSE 0 END) - (CASE WHEN DATENAME(dw, getdate()) 
                      = 'Saturday' THEN 1 ELSE 0 END) AS Days_Open, a.EmployeeWN AS [Issued By], dbo.Assignments.EmployeeName AS [Assigned To], b.Status_EN, 
                      b.Status_FR
FROM         dbo.NCF_General a INNER JOIN
                      dbo.Status b ON a.Status = b.StatusNo INNER JOIN
                      dbo.NCF_Admin c ON a.NCFNo = c.NCFNo INNER JOIN
                      dbo.Assignments ON c.AssignedTo = dbo.Assignments.EmployeeNo
WHERE     (b.Status_EN LIKE '%OPEN%')
ORDER BY days_open DESC</CommandText>
      </Query>
    </DataSet>
  </DataSets>
