      <Query>
        <DataSourceName>NCF</DataSourceName>
        <CommandText>SELECT     dbo.NCF_General.NCFNo, dbo.NCF_General.CategoryNo, dbo.Status.Status_EN, dbo.AD_LCL_USERS.displayName AS IssuedBy, 
                      dbo.NCF_General.IssueDate, dbo.NCF_Admin.FollowUpDate, dbo.NCF_Admin.TargetCloseOutDate, dbo.NCF_Admin.CloseOutDate, 
                      dbo.NCF_General.Deficiency, dbo.NCF_General.ActionT, dbo.NCF_General.Cause, dbo.NCF_General.Suggestions, 
                      dbo.Assignments.EmployeeName AS AssignedTo, dbo.QRY_Customers.CUSTOMERNAME, dbo.NCF_General.OrderNo, dbo.NCF_General.VendorNo, 
                      dbo.NCF_General.PONo, dbo.VENPHY01.LRRKTX AS Vendor_Name
FROM         dbo.QRY_Customers INNER JOIN
                      dbo.NCF_General INNER JOIN
                      dbo.Contact ON dbo.NCF_General.ContactNo = dbo.Contact.ContactNo INNER JOIN
                      dbo.Status ON dbo.NCF_General.Status = dbo.Status.StatusNo INNER JOIN
                      dbo.NCF_Admin ON dbo.NCF_General.NCFNo = dbo.NCF_Admin.NCFNo ON dbo.QRY_Customers.CUSTOMERNO = dbo.Contact.CustomerNo INNER JOIN
                      dbo.AD_LCL_USERS ON dbo.NCF_General.EmployeeWN = dbo.AD_LCL_USERS.sAMAccountName LEFT OUTER JOIN
                      dbo.VENPHY01 ON dbo.NCF_General.VendorNo = dbo.VENPHY01.LRRCCD LEFT OUTER JOIN
                      dbo.Assignments ON dbo.NCF_Admin.AssignedTo = dbo.Assignments.EmployeeNo
WHERE     (dbo.Status.Status_EN LIKE CASE WHEN @status = 'ALL' THEN '%' ELSE @status END) AND (dbo.NCF_General.IssueDate &gt;= @IssueStartDate) AND 
                      (dbo.NCF_General.IssueDate &lt;= @IssueEndDate) AND (dbo.NCF_General.VendorNo LIKE CASE WHEN @Vendor = 'ALL' THEN '%' ELSE @Vendor END) 
                      AND (dbo.NCF_Admin.AssignedTo LIKE CASE WHEN @AssignedTo = 0 THEN '%' ELSE @AssignedTo END) AND 
                      (dbo.NCF_General.ContactNo LIKE CASE WHEN @Customer = 0 THEN '%' ELSE @Customer END)
ORDER BY dbo.NCF_General.NCFNo</CommandText>
        <QueryParameters>
          <QueryParameter Name="@status">
            <Value>=Parameters!status.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IssueStartDate">
            <Value>=Parameters!IssueStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IssueEndDate">
            <Value>=Parameters!IssueEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Vendor">
            <Value>=Parameters!Vendor.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AssignedTo">
            <Value>=Parameters!AssignedTo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Customer">
            <Value>=Parameters!Customer.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
