      <Query>
        <DataSourceName>NCF</DataSourceName>
        <CommandText>SELECT     dbo.NCF_General.NCFNo, dbo.NCF_General.CategoryNo, dbo.Status.Status_EN, dbo.NCF_General.IssueDate, dbo.NCF_Admin.FollowUpDate, 
                      dbo.NCF_Admin.TargetCloseOutDate, dbo.NCF_Admin.CloseOutDate, dbo.NCF_General.Deficiency, dbo.NCF_General.ActionT, dbo.NCF_General.Cause, 
                      dbo.NCF_General.Suggestions, dbo.Assignments.EmployeeName AS AssignedTo, dbo.QRY_Customers.CUSTOMERNAME, dbo.NCF_General.OrderNo, 
                      dbo.NCF_General.PONo, dbo.NCF_General.VendorNo, dbo.VENPHY01.LRRKTX AS Vendor_Name, dbo.NCF_General.EmployeeWN
FROM         dbo.QRY_Customers INNER JOIN
                      dbo.NCF_General INNER JOIN
                      dbo.Contact ON dbo.NCF_General.ContactNo = dbo.Contact.ContactNo INNER JOIN
                      dbo.Status ON dbo.NCF_General.Status = dbo.Status.StatusNo INNER JOIN
                      dbo.NCF_Admin ON dbo.NCF_General.NCFNo = dbo.NCF_Admin.NCFNo ON 
                      dbo.QRY_Customers.CUSTOMERNO = dbo.Contact.CustomerNo LEFT OUTER JOIN
                      dbo.VENPHY01 ON dbo.NCF_General.VendorNo = dbo.VENPHY01.LRRCCD LEFT OUTER JOIN
                      dbo.Assignments ON dbo.NCF_Admin.AssignedTo = dbo.Assignments.EmployeeNo
WHERE     (dbo.Status.StatusNo = 2) OR
                      (dbo.Status.StatusNo = 3)
ORDER BY dbo.NCF_General.NCFNo</CommandText>
      </Query>
